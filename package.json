{
  "name": "my-store",
  "private": true,
  "pnpm": {
    "onlyBuiltDependencies": [
      "@medusajs/telemetry",
      "@swc/core",
      "esbuild",
      "msgpackr-extract",
      "protobufjs",
      "sharp"
    ],
    "overrides": {
      "zod": "3.25.76"
    }
  },
  "scripts": {
    "dev": "pnpm --filter './apps/**' run dev",
